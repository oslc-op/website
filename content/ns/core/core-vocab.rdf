    <dcterms:description rdf:parseType="Literal">All vocabulary URIs defined in the OSLC Core namespace.</dcterms:description>
    <rdfs:label>The OSLC Core Vocabulary</rdfs:label>
    <dcterms:license rdf:resource="http://www.apache.org/licenses/LICENSE-2.0"/>
