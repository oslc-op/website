    <dcterms:title>The OSLC Core Vocabulary</dcterms:title>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2020-09-17</dcterms:issued>
    <dcterms:isPartOf rdf:resource="https://docs.oasis-open-projects.org/oslc-op/core/v3.0/ps01/oslc-core.html"/>
