                    oslc:occurs oslc:Zero-or-one ;
                    oslc:propertyDefinition oslc_promcode:sequenceNumber ;
                    oslc:valueType xsd:decimal ;
                    dcterms:description "A unique number assigned to this resource which represents a position in the sequential order of all the <code>Issue</code> resources in the project." 
                  ];
    oslc:property [ a oslc:Property ;
                    oslc:name "raisedBy" ;
                    oslc:occurs oslc:Zero-or-many ;
