    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">Very short label for use in menu items.</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#label"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>label</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">A Creation Factory MAY provide Resource Shapes that describe shapes of resources that may be created.</dcterms:description>
        <oslc:valueType rdf:resource="http://open-services.net/ns/core#Resource"/>
        <oslc:representation rdf:resource="http://open-services.net/ns/core#Reference"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
