  <oslc:ResourceShape rdf:about="http://open-services.net/ns/core/shapes/3.0#ExtendedErrorShape">
    <dcterms:title>OSLC Core ExtendedError Shape</dcterms:title>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">A resource giving more information on the error SHOULD be of an HTML content-type.</dcterms:description>
        <oslc:valueType rdf:resource="http://open-services.net/ns/core#Resource"/>
        <oslc:representation rdf:resource="http://open-services.net/ns/core#Reference"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#moreInfo"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>moreInfo</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">If present and set to 'alternate' then indicates that work-around is provided, behavior for other values is undefined.</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#rel"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>rel</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">Values MUST be expressed in relative length units as defined in the W3C Cascading Style Sheets Specification (CSS 2.1) Em and ex units are interpreted relative to the default system font (at 100% size).</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#hintWidth"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>hintWidth</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">Values MUST be expressed in relative length units as defined in the W3C Cascading Style Sheets Specification (CSS 2.1) Em and ex units are interpreted relative to the default system font (at 100% size).</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#hintHeight"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>hintHeight</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:describes rdf:resource="http://open-services.net/ns/core#ExtendedError"/>
  </oslc:ResourceShape>
  <oslc:ResourceShape rdf:about="http://open-services.net/ns/core/shapes/3.0#ErrorShape">
    <dcterms:title>OSLC Core Error Shape</dcterms:title>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">The HTTP status code reported with the error.</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#statusCode"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Exactly-one"/>
        <oslc:name>statusCode</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">An informative message describing the error that occurred.</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#message"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Exactly-one"/>
        <oslc:name>message</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">Extended error information.</dcterms:description>
        <oslc:valueType rdf:resource="http://open-services.net/ns/core#AnyResource"/>
        <oslc:representation rdf:resource="http://open-services.net/ns/core#Either"/>
        <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</oslc:readOnly>
        <oslc:range rdf:resource="http://open-services.net/ns/core#ExtendedError"/>
        <oslc:propertyDefinition rdf:resource="http://open-services.net/ns/core#extendedError"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>extendedError</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">Optional indication of when the error was detected.</dcterms:description>
        <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <oslc:propertyDefinition rdf:resource="http://purl.org/dc/terms/created"/>
        <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-one"/>
        <oslc:name>created</oslc:name>
      </oslc:Property>
    </oslc:property>
    <oslc:property>
      <oslc:Property>
        <dcterms:description rdf:parseType="Literal">A reference to any resources that are the subject of this error.</dcterms:description>
        <oslc:valueType rdf:resource="http://open-services.net/ns/core#AnyResource"/>
